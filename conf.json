{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "Pizza-Hub/",
            "Pizza-Hub/utils/",
            "Pizza-Hub/Models/",
            "Pizza-Hub/lib/",
            "Pizza-Hub/handlers/"
        ],
        "exclude": [
            "Pizza-Hub/https",
            "Pizza-Hub/public",
            "Pizza-Hub/.db",
            "Pizza-Hub/.logs",
            "Pizza-Hub/.vscode"
        ],
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "recurse": "true",
        "private": "true"
    }
}
